{
  "compilerOptions": {
      "strict": true,
      "allowSyntheticDefaultImports": true,
      "esModuleInterop": true,
      "isolatedModules": true,
      "jsx": "preserve",
      "jsxImportSource": "solid-js",
      "target": "ESNext",
      "module": "ESNext",
      "moduleResolution": "node",
      "noEmit": true,
      "types": ["solid-start/env"],
      "baseUrl": "./",
      "paths": {
          "~/*": ["./src/*"]
      }
  }
}
